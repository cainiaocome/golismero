[Core]
Name = Bruteforce URL discovery

[Documentation]
Description = Tries to discover URLs not linked anywhere in the page using a brute force approach.
Author = GoLismero project team
Version = 0.1
Website = https://github.com/cr0hn/golismero/
Copyright = Copyright (C) 2011-2013 GoLismero Project
License = GNU Public License


